template(name='fullscreen')
    +yield
    if in_dev
        +admin_footer
      

      
template(name='admin_footer')
    .ui.fluid.inverted.segment
        .ui.inline.header 
            i.shield.icon
            |admin footer
        .ui.inline.header #{docs.count} docs
        .ui.stackable.padded.grid
            .five.wide.column
                .ui.small.inverted.list.smallscroll
                    each docs
                        .item 
                            .content 
                                .header
                                    |#{title} 
                                .ui.blue.label
                                    |#{model}
                                |#{when} #{_author_username} 
                                .ui.label #{app}
                                +print_this
                                +remove_button
            .five.wide.column
                .ui.inline.inverted.header #{results.count} results
                .ui.small.inverted.list.smallscroll
                    each results
                        .item 
                            .content 
                                .header
                                    |#{title} 
                                .ui.label model:#{model}
                                +print_this
      



template(name='not_found')
    .ui.center.aligned.basic.segment
        .ui.header 
            // i.question.icon
            |'#{current_route}'
            |not found
        .ui.big.basic.ciruclar.button.browser_back
            i.undo.icon
            |back
            
                  
                  
                  
template(name='print_this')
    i.circular.large.code.link.icon.print_this
template(name='remove_button')
    i.circular.large.remove.link.icon.print_this